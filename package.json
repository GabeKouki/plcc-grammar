{
  "name": "plcc-grammar",
  "displayName": "PLCC Grammar",
  "publisher": "gabekouki",
  "author": "Gabe Kouki",
  "description": "Syntax highlighting for PLCC .grammar files",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.105.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GabeKouki/plcc-grammar"
  },
  "keywords": [
    "PLCC",
    "grammar",
    "syntax",
    "highlighting"
  ],
  "categories": [
    "Programming Languages"
  ],
  "icon": "images/PLCC.png",
  "contributes": {
    "languages": [{
      "id": "plcc",
      "aliases": ["PLCC Grammar", "plcc"],
      "extensions": [".grammar"],
      "filenames": ["grammar"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "plcc",
      "scopeName": "source.plcc",
      "path": "./syntaxes/plcc.tmLanguage.json"
    }],
    "themes": [{
      "label": "PLCC Natural Theme",
      "uiTheme": "vs-dark",
      "path": "./themes/plcc-natural-theme.json"
    }],
    "configurationDefaults": {
      "[plcc]": {
        "editor.tabSize": 2
      },
      "files.associations": {
        "*grammar": "plcc",
        "grammar": "plcc"
      }
    }
  }
}